<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
                             http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <servlet>
        <servlet-name>add</servlet-name>
        <servlet-class>com.set1.AddServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>add</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>t</servlet-name>
        <servlet-class>com.set1.test</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>t</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>sq</servlet-name>
        <servlet-class>com.set1.SqServlet</servlet-class> 
    </servlet>

    <servlet-mapping>
        <servlet-name>sq</servlet-name>
        <url-pattern>/sq</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>com.set1.MyServlet</servlet-class> 
        
        <init-param>
        <param-name>Dad</param-name>
        <param-value>Ambadas</param-value>
        </init-param>
    </servlet>

    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
    
    <context-param>
    		<param-name>Name</param-name>
    		<param-value>Srinivas</param-value>
    </context-param>
    <context-param>
    		<param-name>Phone</param-name>
    		<param-value>Realme</param-value>
    </context-param>

</web-app>
